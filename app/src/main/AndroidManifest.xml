<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.miwik1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NumbersActivity"
                  android:label="@string/category_number"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".PhraseActivity"
                  android:label="@string/category_phrase"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".FamilyActivity"
                  android:label="@string/category_family"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".ColorsActivity"
                  android:label="@string/category_colors"
                  android:parentActivityName=".MainActivity"/>
    </application>

</manifest>